        "glob": "^11.0.0",
        "html2canvas": "^1.4.1",
        "http": "^0.0.1-security",
        "http-server": "^14.1.1",
        "jquery": "^3.7.1",
        "jspdf": "^2.5.1",
        "jsplumb": "^2.15.6",
        "leaflet": "^1.9.4",
