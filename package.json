        "jest-environment-jsdom": "^29.7.0",
        "leaflet": "^1.9.4",
        "prettier": "^2.8.8"
    },
    "jest": {
        "testEnvironment": "jsdom",
        "setupFiles": [
            "<rootDir>/Client/js/Test/jest.setup.js"
        ],
        "moduleNameMapper": {
            "^@/(.*)$": "<rootDir>/Client/js/$1"
        },
        "transform": {
            "^.+\\.js$": "babel-jest"
        }
    },
    "babel": {
